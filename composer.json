{
  "name": "dataswitcher/dataswitcher_logistics_client",
  "type": "library",
  "description": "A PHP Client for the Dataswitcher Logistics API",
  "keywords": [
    "api",
    "php",
    "dataswitcher"
  ],
  "require": {
    "php": "~8.0",
    "guzzlehttp/guzzle": "~7.0",
    "symfony/cache": "^5.2",
    "swisnl/json-api-client": "^2.3.1",
    "php-http/guzzle7-adapter": "1.0",
    "http-interop/http-factory-guzzle": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Dataswitcher\\": "src/Dataswitcher"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "pestphp/pest-plugin": true
    }
  },
  "require-dev": {
    "vlucas/phpdotenv": "^5.5",
    "pestphp/pest": "^1.22",
    "mockery/mockery": "^1.5",
    "symfony/var-dumper": "^6.0"
  }
}
